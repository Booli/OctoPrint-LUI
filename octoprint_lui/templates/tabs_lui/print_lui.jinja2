<div class="tab open" id="print">
    <div class="content">
        <div class="status_block print_status">
            <div class="Table">
                <div class="Table-row Table-header">
                    <div class="Table-item">Print Job</div>
                </div>
                <div class="Table-row">
                    <div class="Table-item Table-item-grow-4"><i class="fa fa-cube" style="font-size: 12em; color: lightgrey"></i></div>
                    <div class="Table-item Table-item-grow-5 Table-nested">
                        <div class="Table-row">
                            <div class="Table-item">Job name:</div>
                            <div class="Table-item" data-bind="text: filename"></div>
                        </div>
                        <div class="Table-row">
                            <div class="Table-item">Estimated time:</div>
                            <div class="Table-item" data-bind="text: estimatedPrintTimeString">1h</div>
                        </div>
                        <div class="Table-row">
                            <div class="Table-item">Time left:</div>
                            <div class="Table-item" data-bind="text: printTimeLeftString">59m</div>
                        </div>
                        <div class="Table-row">
                            <div class="Table-item">Print time:</div>
                            <div class="Table-item" data-bind="text: printTimeString">1m</div>
                        </div>
                        <div class="Table-row">
                            <div class="Table-item">
                                <ul class="button-area">
                                    <li class="button" data-bind="click: function(){showFileSelectFlyout()}, css: {disabled: isPrinting() || !loginState.loggedIn()} " id="file_select"><a href="#"><span><i class="fa fa-file"></i>Select print job</span></a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div> 
        </div>
        <div class="status_block print_control" data-bind="visible: !isPrinting()">
            <div class="Table">
                <div class="Table-row Table-header">
                    <div class="Table-item">Print Control</div>
                </div>
                <div class="Table-row">
                    <div class="Table-item">
                        <ul class="button-area">
                            <li class="button secondary-button"><a href="#"><span>Pre-heat</span></a></li>
                        </ul>
                    </div>
                    <div class="Table-item">
                        <ul class="button-area">
                            <li class="button ok-button" data-bind="click: print, css: { disabled: !isOperational() || !fileSelected() || !loginState.loggedIn()}"><a href="#"><span><i class="fa fa-play"></i>Start print</span></a></li>
                        </ul>
                    </div>
                </div>
            </div> 
        </div>
        <div id="temp" style="display: none;">

        </div>
        <div class="status_block print_progress" data-bind="visible: isPrinting()">
            <div class="Table">
                <div class="Table-row Table-header">
                    <div class="Table-item">Print Status</div>
                </div>
                <div class="Table-row">
                    <div class="Table-item">Status</div>
                    <div class="Table-item Table-item-grow-7">
                        <div class="progress">
                            <span class="bg-orange" style="width: 40%">Pre-heating</span>
                        </div>
                    </div>
                </div>
                <div class="Table-row">
                    <div class="Table-item">Total</div>
                    <div class="Table-item Table-item-grow-7">
                        <div class="progress">
                            <span class="bg-main" style="width: 10%"></span>
                        </div>
                    </div>
                </div>
                <div class="Table-row">
                    <div class="Table-item">
                        <ul class="button-area">
                            <li class="button"><a href="#"><span ><i class="fa fa-pause"></i>Pause</span></a></li>
                        </ul>
                    </div>
                    <div class="Table-item">
                        <ul class="button-area">
                            <li class="button failed-button" data-bind="click: cancel"><a href="#"><span><i class="fa fa-stop"></i>Cancel</span></a></li>
                        </ul>
                    </div>
                </div>
            </div> 
        </div>
        <!-- ko allowBindings: false -->
        <div id="filament_status" class="status_block print_filament">
            <div class="Table">
                <div class="Table-row Table-header">
                    <div class="Table-item">Left</div>
                    <div class="Table-item">Right</div>
                </div>
                <div class="Table-row">
                    <div class="Table-row">
                        <div class="Table-item">Material</div>
                        <!-- ko if: settings.settings.plugins.lui.filaments()[1]-->
                        <div class="Table-item" data-bind="text: settings.settings.plugins.lui.filaments()[1].material.name"></div>
                        <!-- /ko -->
                    </div>
                    <div class="Table-row">
                        <div class="Table-item">Material</div>
                         <!-- ko if: settings.settings.plugins.lui.filaments()[0]-->
                        <div class="Table-item" data-bind="text: settings.settings.plugins.lui.filaments()[0].material.name"></div>
                        <!-- /ko -->
                    </div>
                </div>
                <div class="Table-row">
                    <div class="Table-row">
                        <div class="Table-item">Filament left</div>
                         <!-- ko if: settings.settings.plugins.lui.filaments()[1]-->
                        <div class="Table-item"><span data-bind="text: settings.settings.plugins.lui.filaments()[1].amountLeft"></span>mm</div>
                         <!-- /ko -->
                    </div>
                    <div class="Table-row">
                        <div class="Table-item">Filament left</div>
                        <!-- ko if: settings.settings.plugins.lui.filaments()[0]-->
                        <div class="Table-item"><span data-bind="text: settings.settings.plugins.lui.filaments()[0].amountLeft"></span>mm</div>
                         <!-- /ko -->
                    </div>
                </div>
                <div class="Table-row">
                    <div class="Table-row">
                        <div class="Table-item">Filament needed</div>
                        <div class="Table-item">2000mm</div>
                    </div>
                    <div class="Table-row">
                        <div class="Table-item">Filament needed</div>
                        <div class="Table-item">4000mm</div>
                    </div>
                </div>
                <div class="Table-row" data-bind="visible: !printerState.isPrinting()">
                    <div class="Table-row">
                        <div class="Table-item">
                            <ul class="button-area">
                                <li class="button secondary-button" data-bind="click: function(){showFilamentChangeFlyout(1)}"><a href="#"><span><i class="fa fa-refresh"></i>Change filament</span></a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="Table-row">
                        <div class="Table-item">
                            <ul class="button-area">
                                <li class="button secondary-button" data-bind="click: function(){showFilamentChangeFlyout(0)}"><a href="#"><span><i class="fa fa-refresh"></i>Change filament</span></a></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div> 
        </div>
        <!-- /ko -->
    </div>
</div>