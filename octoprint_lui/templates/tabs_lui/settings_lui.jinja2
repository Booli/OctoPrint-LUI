                    <li class="topic_dash">
                        <a href="">
                            <i class="fa fa-user"></i>
                        </a>
                    </li>
                    <li class="topic_name">
                        <a href="#topic">
                            <span>Users</span>
                        </a>
                    </li>
                    <li class="topic_info">
                        <a href="#topic_info">
                            <i class="fa fa-2x fa-angle-right"></i>
                        </a>
                    </li>
                </ul>
            </div><!-- Users -->
            <!-- Wireless -->
            <div class="settings_topic" data-bind="click: function() {showSettingsTopic('settingsWireless')}">
                <ul class="topic_main">
                    <li class="topic_dash">
                        <a href="">
                            <i class="fa fa-wifi"></i>
                        </a>
                    </li>
                    <li class="topic_name">
                        <a href="#topic">
                            <span>Wireless</span>
                        </a>
                    </li>
                    <li class="topic_info">
                        <a href="#topic_info">
                            <i class="fa fa-2x fa-angle-right"></i>
                        </a>
                    </li>
                </ul>
            </div><!-- Wireless -->
            <!-- Advanced -->
            <div class="settings_topic" data-bind="click: function() {showSettingsTopic('settingsAdvanced')}">
                <ul class="topic_main">
                    <li class="topic_dash">
                        <a href="">
                            <i class="fa fa-plus"></i>
                        </a>
                    </li>
                    <li class="topic_name">
                        <a href="#topic">
                            <span>Advanced</span>
                        </a>
                    </li>
                    <li class="topic_info">
                        <a href="#topic_info">
                            <i class="fa fa-2x fa-angle-right"></i>
                        </a>
                    </li>
                </ul>
            </div><!-- Advanced -->
        </div><!-- /Topics -->
    </div> <!-- /Content -->
</div>
<!-- Printer -->
<script type="text/html" id="settingsPrinter">
    <div class="flyout-header">
        <h4>Printer</h4>
        <span class="flyout-close" onClick="toggleFlyOut()">x</span>
    </div>
    <div class="flyout-body">
        {% include "settings_lui/printer_lui.jinja2" %}
         <ul class="button-area">
            <li class="button"><a href="#save" data-bind="click: function() { saveData(undefined, $root.hide) }"><span>Save</span></a>
        </ul>
    </div>
</script>
