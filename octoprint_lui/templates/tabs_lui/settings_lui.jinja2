<div class="tab" id="settings">
    <div class="content">
        <div class="settings_topics">
            <!-- Printer -->
            <div class="settings_topic" data-bind="click: function() {showSettingsTopic('settingsPrinter')}">
                <ul class="topic_main">
                    <li class="topic_dash">
                        <a href="">
                            <i class="fa fa-print"></i>
                        </a>
                    </li>
                    <li class="topic_name">
                        <a href="#topic">
                            <span>Printer</span>
                        </a>
                    </li>
                    <li class="topic_info">
                        <a href="#topic_info">
                            <i class="fa fa-2x fa-angle-right"></i>
                        </a>
                    </li>
                </ul>
            </div><!-- Printer -->
            <!-- Materials -->
            <div class="settings_topic" data-bind="click: function() {showSettingsTopic('settingsMaterials')}">
                <ul class="topic_main">
                    <li class="topic_dash">
                        <a href="">
                            <i class="fa fa-sun-o"></i>
                        </a>
                    </li>
                    <li class="topic_name">
                        <a href="#topic">
                            <span>Materials</span>
                        </a>
                    </li>
                    <li class="topic_info">
                        <a href="#topic_info">
