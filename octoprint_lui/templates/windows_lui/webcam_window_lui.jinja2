<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="user-scalable=no">
    <title>OctoPrint - LUI</title>
        {% include 'stylesheets_lui.jinja2' %}
        {% include 'initscript_lui.jinja2' %}
</head>
<body>
    <div class="app-wrap">
     <header id="header" class="app-header">
			<div class="header_logo" data-bind="click: function() { location.reload(); }">
				<a href="#" class="logo">
					<img src="{{ url_for("plugin.lui.static", filename="img/logo.png") }}" width="60%" height="60%">
				</a>
			</div>
		</header>
        <section id="player">
		<video width="1280" height="720" autoplay>
			<source src="http://192.168.0.133/webcam/?action=stream">
		</video>
		</section>
    </div>
    {% include 'javascript_lui.jinja2' %}
</body>
</html>
