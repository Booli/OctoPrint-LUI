<div id="maintenance_control" class="Table active">
  <div class="Table-header Table-row">
    <div class="Table-item">{{ _('Maintenance') }}</div>
  </div>
  <div class="Table-row">
    <div class="Table-item">
      <ul class="button-area">
        <li class="button" data-bind="touchClick: function () { headMaintenancePosition() }">
          <a href="#">
            <span>{{ _('Head maintenance position') }}</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
  <div class="Table-row">
    <div class="Table-item">
      <ul class="button-area">
        <li class="button" data-bind="touchClick: function () { cleanBedPosition() }">
          <a href="#">
            <span>{{ _('Clean bed position') }}</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
  <div class="Table-row">
    <div class="Table-item">
      <ul class="button-area">
        <li class="button" data-bind="touchClick: function () { setFilamentAmount() } ">
          <a href="#">
            <span>{{ _('Set filament amount') }}</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
  {% if printer_profile["extruderCalibration"] %} <!-- for now only available for Bolt -->
  <div class="Table-row">
    <div class="Table-item">
      <ul class="button-area">
        <li class="button" data-bind="touchClick: function () { calibrateExtruders() }">
          <a href="#">
            <span>{{ _('Calibrate extruders') }}</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
  {% endif %}
  <div class="Table-row">
    <div class="Table-item">
      <ul class="button-area">
        <li class="button" data-bind="touchClick: function () { calibrateBed() }">
          <a href="#">
            <span>{{ _('Calibrate bed') }}</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
  <div class="Table-row">
    <!-- ko foreach: filament.filaments().slice(0).reverse() -->
    <div class="Table-item">
      <ul class="button-area">
        <li class="button">
          <a href="#" data-bind="touchClick: function () { $root.beginPurgeWizard(tool()); }, css: {disabled: materialProfileName() == 'None' || $root.printerState.isPrinting || !$root.printerState.isOperational() || !$root.loginState.loggedIn() } ">
              <span data-bind="html: $root.getPurgeButtonContents(tool())"></span>
          </a>
        </li>
      </ul>
    </div>
    <!-- /ko -->
  </div>
  <div class="Table-row">
    <div class="Table-item">
      <ul class="button-area">
        <li class="button" data-bind="touchClick: function () { logFiles() }">
          <a href="#">
            <span>{{ _('Log files') }}</span>
          </a>
        </li>
      </ul>
    </div>
  </div>
</div>

